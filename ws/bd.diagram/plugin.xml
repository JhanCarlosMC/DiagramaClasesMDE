   <extension point="org.eclipse.ui.navigator.navigatorContent" id="navigator-content">
      <?gmfgen generated="true"?>
      <navigatorContent
            id="bd.diagram.resourceContent" 
            name="%navigatorContentName" 
            priority="normal" 
            contentProvider="bd.diagram.navigator.BdNavigatorContentProvider" 
            labelProvider="bd.diagram.navigator.BdNavigatorLabelProvider"
            icon="icons/obj16/BdDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="bd_diagram"/>
               </and>
               <instanceof value="bd.diagram.navigator.BdAbstractNavigatorItem"/>
           	   <adapt type="org.eclipse.gmf.runtime.notation.View">
           	      <test property="bd.diagram.isShortcut"/>
           	   </adapt>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
         	   <instanceof value="bd.diagram.navigator.BdAbstractNavigatorItem"/>
           	   <adapt type="org.eclipse.gmf.runtime.notation.View">
           	      <test property="bd.diagram.isShortcut"/>
           	   </adapt>
            </or>
         </possibleChildren>
         <commonSorter 
               id="bd.diagram.navigatorSorter" 
               class="bd.diagram.navigator.BdNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="bd_diagram"/>
                  </and>
                  <instanceof value="bd.diagram.navigator.BdAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            id="bd.diagram.domainResourceContent" 
            name="%domainNavigatorContentName" 
            priority="normal" 
            contentProvider="bd.diagram.navigator.BdDomainNavigatorContentProvider" 
            labelProvider="bd.diagram.navigator.BdDomainNavigatorLabelProvider"
            icon="icons/obj16/BdDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="bd"/>
               </and>
               <instanceof value="bd.diagram.navigator.BdDomainNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof value="bd.diagram.navigator.BdDomainNavigatorItem"/>
         </possibleChildren>
      </navigatorContent>
      <actionProvider
            id="bd.diagram.navigatorActionProvider"
            class="bd.diagram.navigator.BdNavigatorActionProvider">
         <enablement>
            <or>
               <instanceof value="bd.diagram.navigator.BdAbstractNavigatorItem"/>
           	   <adapt type="org.eclipse.gmf.runtime.notation.View">
           	      <test property="bd.diagram.isShortcut"/>
           	   </adapt>
            </or>
         </enablement>
      </actionProvider>
   </extension>
   
