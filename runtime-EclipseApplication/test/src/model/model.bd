<?xml version="1.0" encoding="ASCII"?>
<bd:ModelFactory xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:bd="http://bd">
  <lstSchema>
    <listTables name="Universidad">
      <primaryKey type="INT" name="idU" autoIncrement="true"/>
      <listColumns type="VARCHAR" name="nombre"/>
      <listColumns type="VARCHAR" name="direccion"/>
    </listTables>
    <listTables name="Profesor">
      <primaryKey type="INT" name="idProfesor" autoIncrement="true"/>
      <listColumns type="VARCHAR" name="nroTarjetaProfesional"/>
      <listColumns type="INT" name="aniosExperiencia"/>
      <listColumns type="VARCHAR" name="especialidad"/>
      <listForeignKey type="INT" name="documentoIdentidad" autoIncrement="true"/>
    </listTables>
    <listTables name="Estudiante">
      <primaryKey type="INT" name="idEstudiante" autoIncrement="true"/>
      <listColumns type="VARCHAR" name="colegioEgresado"/>
      <listColumns type="VARCHAR" name="sisben"/>
      <listColumns name="semestre"/>
      <listForeignKey type="INT" name="documentoIdentidad" autoIncrement="true"/>
    </listTables>
    <listTables name="Programa">
      <primaryKey type="INT" name="idPrograma" autoIncrement="true"/>
      <listColumns type="VARCHAR" name="nombre"/>
      <listColumns name="codigo"/>
      <listForeignKey type="INT" name="idEstudiante" autoIncrement="true" tableReferenced="//@lstSchema.0/@listTables.2"/>
      <listForeignKey type="INT" name="idU" autoIncrement="true" tableReferenced="//@lstSchema.0/@listTables.0"/>
    </listTables>
    <listTables name="Persona">
      <primaryKey type="INT" name="documentoIdentidad" autoIncrement="true"/>
      <listColumns type="VARCHAR" name="nombre"/>
      <listColumns type="INT" name="edad"/>
      <listForeignKey type="INT" name="idU" autoIncrement="true" tableReferenced="//@lstSchema.0/@listTables.0"/>
    </listTables>
  </lstSchema>
</bd:ModelFactory>
